{
  "defaultConnection": "default",
  "connections": {
    "default": {
      "connector": "strapi-hook-bookshelf",
      "settings": {
        "client": "postgres",
        "uri": "",
        "host": "127.0.0.1",
        "port": 5432,
        "database": "${process.env.DB_NAME}",
        "username": "${process.env.DB_USER}",
        "password": "${process.env.DB_PASS}"
      },
      "options": {
        "ssl": "false",
        "authenticationDatabase": ""
      }
    }
  }
}
